/* pages/my/index.wxss */
.my{
  min-height: 100%;
}
.title{
  font-size: 14px;
}
.top{
  display: flex;
  align-items: center;
  justify-content: center;
  flex-direction:  column;
  padding: 40rpx 0;
  background: #fff;
  font-size: 12px;
}
.top image{
  width: 170rpx;
  height: 170rpx;
  border-radius: 50%;
  border: 5rpx solid #fff;
  box-shadow: 0 0 30rpx #fff8f1;
}
.top .name{
  padding: 25rpx 0;
  font-size: 14px;
  color: #666;
}
.my-order,.my-cart{
  margin-top: 20rpx;
}
.all-order{
  height: 85rpx;
}
.all-order .right{
  font-size: 12px;
  color: #666;
}
.order-item text{
  font-size: 12px;
}
.link-item{
  padding: 0 30rpx;
}
.my-cart,.call{
  height: 95rpx;
}
.my-cart .right{
  padding-right: 0;
}
.my-cart .right view{
  display: inline-block;
  padding: 0 15rpx;
  background: #ff4444;
  color: #fff;
  font-size: 12px;
  border-radius: 32rpx;
  line-height: 32rpx;
}
.link:after{
  right: 30rpx;
}
.order-list{
  display: flex;
  align-items: center;
  justify-content:  space-around;
  padding: 40rpx 0;
  background: #fff;
  font-size: 12px;
}

.order-item:nth-child(1) .icon{
  background-image: url(data:image/png;base64,)
}
.order-item:nth-child(2) .icon{
  background-image: url(data:image/png;base64,)
}
 .order-item:nth-child(3) .icon{
   background-image: url(data:image/png;base64,) 
} 
.order-item:nth-child(4) .icon{
  background-image: url(data:image/png;base64,)
}
.order-item .icon{
  width: 48rpx;
  height: 48rpx;
  background-size: cover;
  margin: 0 auto 10rpx auto;
}
