/* pages/cart/index.wxss */
.cart{
  padding-bottom: 100rpx;
}
.title{
  display: flex;
  align-items: center;
  justify-content: space-between;
  background: #fff;
  padding: 0 22rpx;
  border-bottom: 1px solid #eee;
  height: 90rpx;
}
.title .left{
  display: flex;
  align-items: center;
}
.title icon{
  margin-right: 25rpx;
}
.title .name{
  font-size: 14px;
}
.title .icon{
  background: url(data:image/png;base64,);
  background-size: cover;
  width: 28rpx;
  height: 25rpx;
  margin-right: 10rpx;
}
.title .edit{
  float: right;
  font-size: 14px;
  color: #999;
}
.cart-shop-item{
  padding-left: 22rpx;
  background: #fff;
  overflow: hidden;
}
.slider-box{
  display: flex;
  align-items: center;
  
}
.slider-box.slider{
  transition: all 0.1s; 
}
.slider-box.active{
  transform: translateX(-114rpx);
}
.cart-shop-item .select{
  width: 70rpx;
  height: 204rpx;
  line-height: 204rpx;
}
.cart-shop-item .right{
  flex: 1;
  display: flex;
  padding: 15rpx 0 15rpx 0;
  align-items: center;
  border-bottom: 1px solid #eee;
}
.cart-shop-item image{
  width: 170rpx;
  height: 170rpx;
  border: 1px solid #eee;
}
.cart-shop-item .close-box .info{
  flex: 1;
  margin-left: 20rpx;
  padding-right:22rpx;
}
.cart-shop-item .close-box .info-item{
  display: flex;
  justify-content:  space-between;
}
.close-box .info-item .size{
  color: #999;
}
.close-box .info-item .price{
  color: #ff4343;
  font-size:15px;
}
.close-box .info-item .number{
  color: #999;
  font-size:12px;
}
.cart-shop-item .close-box .name{
  flex: 1;
  overflow: hidden; 
  text-overflow: elipsis; 
  display:-webkit-box; 
  -webkit-line-clamp: 2;
   -webkit-box-orient:vertical;
   font-size: 14px;
}
.cart-shop-item .close-box button{
  padding:0;
  width:110rpx;
  background: #ff4343;
  color: #fff;
  text-align:center;
  margin: -15rpx -125rpx -15rpx 20rpx;
  font-size: 13px;
  height: 204rpx;
  line-height: 204rpx;
  border-radius: 0;
}
.open-box .info{
  flex: 1;
  margin-left: 20rpx;
}
.open-box .price{
  display: flex;
  align-items: center;
  justify-content: space-between;
  margin-top: 20rpx;
}
.open-box .price view{
  color: #999;
  font-size: 12px;
}
.open-box .price text{
  color: #ff4343;
  font-size: 15px;
}
.open-box button{
  padding:0;
  width:110rpx;
  background: #ff4343;
  color: #fff;
  text-align:center;
  margin: -15rpx -0 -15rpx 20rpx;
  font-size: 13px;
  height: 204rpx;
  line-height: 204rpx;
  border-radius: 0;
  border-bottom: 1px solid #fff;
}
.cart-shop-item:last-of-type .open-box button{
  border-bottom: none;
}
.cart-shop-item:last-of-type.info{
  border-bottom: none;
}


.handle{
  justify-content:  space-between;
  padding-left: 22rpx;
}
.handle icon{
  margin-right: 20rpx;
}
.handle .right{
  display: flex;
  align-items: center;
}
.handle .sum{
  text-align: right;
}
.handle .sum view{
  font-size: 14px;
  color: #ff4343;
}
.handle .sum text{
  font-size: 12px;
  color: #999;
}
.handle button{
  width: 220rpx;
  margin: 0 0 0 20rpx;
  line-height: 100rpx;
  background: #ff4343;
  color: #fff;
  font-size: 15px;
  border-radius: 0;
}
.all-select{
  height:100%;
  display:flex;
  align-items:center;
}
.all-select icon{
  vertical-align: middle;
}


.no{
  padding-top: 85rpx;
  display: flex;
  flex-direction: column;
  justify-content: center;
}
.no .icon{
  width: 153rpx;
  height: 119rpx;
  background: url(data:image/png;base64,);
  background-size: cover;
  margin: 0 auto;
}
.no .text{
  font-size: 14px;
  color: #999;
  margin-top: 15rpx;
  margin-bottom: 30rpx;
  text-align: center;
}
.no button{
  height: 60rpx;
  font-size: 12px;
  background: #ff4343;
  color: #fff;
  width: 200rpx;
}