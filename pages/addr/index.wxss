/* pages/addr/index.wxss */
.box{
  padding-bottom: 120rpx;
}
.item{
  padding: 25rpx 22rpx;
  background: #fff;
  display: flex;
  align-items: center;
  justify-content: center;
  border-bottom: 1px solid #eee;
}
.info{
  flex: 1;
  margin-left: 15rpx;
  padding-right: 25rpx;
}
.name{
  font-size:14px;
  margin-bottom:10rpx;
}
.name text{
  margin-left: 20rpx;
}
.addr-txt{
  font-size: 12px;
  color: #666;
}
 .link:after{
  right: 22rpx;
} 
.handle{
  justify-content: center;
  font-size: 16px;
}
.handle .icon{
  background: url(data:image/png;base64,);
  background-size: cover;
  width: 40rpx;
  height: 40rpx;
  margin-right: 10rpx;
}
.btn-sel{
  width:3rem;
  height:1.5rem;
  background: greenyellow;
  color: #fff;
  margin-right: 0.2rem;
  font-size: 0.6rem;
}
.btn-up{
    width:3rem;
  height:1.5rem;
  background: #ff4343;
  color: #fff;
  font-size: 0.6rem;
}
.no{
  padding-top: 150rpx;
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: center;
}
.no .icon{
  width: 205rpx;
  height: 205rpx;
  background-image: url(data:image/png;base64,);
  background-size: cover;
}
.no .txt{
  font-size: 18px;
  color: #666666;
  margin-top: 60rpx;
}
.edit{
  width: 30rpx;
  height: 30rpx;
  background-image: url(data:image/png;base64,);
  background-size: cover;
}