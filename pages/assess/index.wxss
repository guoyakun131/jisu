/* pages/assess/index.wxss */
.top{
  display: flex;
  align-items: center;
  padding: 20rpx 22rpx;
  border-bottom: 1px solid #eee;
  background: #fff;
}
.top image{
  width: 100rpx;
  height: 100rpx;
  border: 1px solid #eee;
}
.top .title{
  margin-left: 20rpx;
  margin-right: 30rpx;
  font-size: 14px;
}

.star{
  margin: 0 17rpx;
  background: url(data:image/png;base64,);
  background-size: cover;
  width: 31rpx;
  height: 30rpx;
  display: inline-block;
  vertical-align: text-top;
}
.star.active{
  background: url(data:image/png;base64,);
  background-size: cover;
}
.middle{
  padding: 0 22rpx;
  background: #fff;
}
textarea{
  height: 235rpx;
  width: 100%;
  padding: 25rpx 10rpx;
  background: #fff;
}
.bottom{
  display: flex;
  align-items: center;
  padding: 40rpx 22rpx;
  background: #fff;
}
.img-box{
  position: relative;
  width: 140rpx;
  height: 140rpx;
  margin: 0 10rpx;
  display: inline-block;
  border: 1px solid #eee;
}
.img-box icon{
  position: absolute;
  top: -15rpx;
  right: -15rpx;
}
.img-box image{
  width: 100%;
  height: 100%;
}
.file-up-img{
  width: 140rpx;
  height: 140rpx;
  margin: 0 10rpx;
  background: url(data:image/png;base64,);
  background-size: cover;
}