/*logo  */
.logo{
  width: 100%;
  height: 80rpx;
  background: #fff; 
  position: relative;
}
.contont-box{
  width: 96%;
  float: left;
  height: 100%;
  margin-left: 4%;
}
.laba-icon image{
  width: 100%;
  height: 100%;
  float: left;
}
.laba-icon{
  float: left;
  width: 30rpx;
  height: 26rpx;
  margin-right: 15rpx;
  margin-top: 25rpx;

}
.notice-text{
  float: left;
  width: 92%;
  height: 50%;
 

}
@keyframes kf-marque-animation{ 0% { transform: translateX(0); } 100% { transform: translateX(-33.3%); } }
.marquee{
    width: 100%;
    height: 44px;
    line-height: 35px;
    background: #fff;
    border: none;
    display: block;
    margin: 0 auto;
    overflow: hidden;
    white-space: nowrap;
    text-overflow: clip;
    position: relative;
    font-size: 28rpx;
}
.marquee-contont{
    display: inline-block;
    position: relative;
    padding-right: 0px;
    animation: kf-marque-animation 5s linear infinite;
    white-space: nowrap;
}

.apple-logo{
  width:180rpx;
  height: 180rpx;
}
.company_name{
  text-indent: 30rpx;
  align-items: center;
  font-size: 30rpx;
  font-weight: bold;
}
/*address and tel  */
.address-tel{
  display: flex;
  flex-direction:row;
  border-top:1px solid #f6f6f6;
  border-bottom: 1px solid #f6f6f6;
  background: #fff;
  height:100rpx;
}
.address{
  width:70%;
  float: left;
  display: flex;
  flex-direction: row;
  position: relative;
}
.hd-zt{
  font-size: 24rpx;
  color:#929292;
}
.tel{
  width: 30%;
  height: 60rpx;
  display: flex;
  flex-direction: row;
  position: relative;
}
.address-img{
  width: 38rpx;
  height: 38rpx;
  margin-right: 20rpx;
}
.city-address{
  display: flex;
  flex-direction: row;
  padding:28rpx 0 0 50rpx;
  box-sizing:border-box;
}
.address_line{
  width: 2rpx;
  height: 45rpx;
  background: #eff0ea;
  margin:30rpx 0 0 110rpx;
}
.tel-img{
  width: 40rpx;
  height: 40rpx;
  position: absolute;
  top: 28rpx;
  left: 20rpx;
  z-index: 999;
}
button[type="default"]{ 
  font-size:24rpx;
  align-items: center;
  color:#929292;
  padding: 18rpx 0 0 ;
  background: #fff;
}
.tab-nav {
  justify-content: space-around;
  align-items: center;
}
.tab-nav .item{
  text-align: center;
  font-size: 12px;
  width: 33.333%;
  border-bottom: 2px solid transparent;
}
.tab-nav .item .number{
  font-size: 16px;
  font-weight: bold;
  color: #333;
}
.tab-nav .item .title{
  color: #999;
}
.tab-nav .item.active{
  border-bottom: 2px solid #ff4444;
}
.tab-nav .item.active .number,.tab-nav .item.active .title{
  color: #ff4444;
}
.tab-box .item{
  margin-top: 10rpx;
  width: 50%;
  float: left;
  padding: 0 5rpx;
}
.tab-box .item:nth-child(2n){
  padding-right: 0;
}
.tab-box .item:nth-child(2n+1){
  padding-left: 0;
}
.tab-box .item image{
  width: 100%;
  height: 370rpx;
}
.tab-box .item .name{
  display: -webkit-box;
  word-break: break-all;
  overflow: hidden; 
  text-overflow: ellipsis;
  -webkit-line-clamp: 2;
  -webkit-box-orient: vertical;
}
.tab-box .item .price{
  margin-top: 10rpx;
  color: #ff4444;
  font-size: 16px;
}
.tab-box .info{
  padding: 10rpx 22rpx 25rpx 22rpx;
  background: #fff;
}
.refer-box{
  position: fixed;
  bottom: 100rpx;
  right: 22rpx;
  text-align: center;
  display: flex;
  flex-direction: row;
}
.refer-box button{
  background: none;
  width: 80rpx;
  height: 80rpx;
  display: flex;
  align-items: center;
  justify-content: center;
  padding: 0;
  margin: 0;
}
.refer-box button view{
  width: 80rpx;
  height: 80rpx;
}
.kf-img{
  width: 80rpx;
  height: 80rpx;
}
.no-shop .img{
  width: 141rpx;
  height: 124rpx;
  background: url(data:image/png;base64,);
  background-size: cover;
  margin: 150rpx auto 40rpx auto;
}
.no-shop .txt{
  color: #999;
  font-size: 16px;
  text-align: center;
}